{
  "include": ["src"],
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "target": "es5",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true
  }
}
